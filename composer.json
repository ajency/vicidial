{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "artesaos/seotools": "^0.12.1",
        "darkaonline/ripcord": "^0.1.6",
        "elasticsearch/elasticsearch": "~6.0",
        "fideloper/proxy": "~4.0",
        "laravel/framework": "5.7.*",
        "league/flysystem-aws-s3-v3": "^1.0",
        "laravel/socialite": "^3.1",
        "laravel/tinker": "~1.0",
        "spatie/laravel-permission": "^2.23"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/ElasticSearch.php",
            "app/Helpers/Helpers.php",
            "app/Helpers/ProductHelpers.php",
            "app/Helpers/CategoryHelpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Ajency\\FileUpload\\" : "packages/ajency/laravel-file-upload-package/src",
            "Ajency\\Comm\\": "packages/ajency/comm/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
